{
  "name": "tableau-web-table-connector",
  "version": "0.5.0",
  "description": "A Web Data Connector for Tableu for loading CSV files, Google Docs Spreadsheets and writing your own loaders for table-like data.",
  "main": "coffee/table_connector.coffee",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "csv": "^0.4.1",
    "jquery": "^2.1.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "coffee-script": "^1.9.1",
    "coffeeify": "^1.0.0",
    "grunt": "^0.4.5",
    "jade": "^1.9.2",
    "jadeify": "^4.1.0"
  },
  "browserify": {
    "extensions": [".coffee", ".hbs"],

    "transform": [
      [
        "coffeeify",
        {
          "bare": false
        }
      ],
      [
        "jadeify",
        {
          "compileDebug": true,
          "pretty": true
        }
      ]
    ]
  },
